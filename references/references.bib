  langid = {english},
  keywords = {Biochemical networks,Metabolic engineering,Numerical simulations},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Biochemical networks;Metabolic engineering;Numerical simulations Subject\_term\_id: biochemical-networks;metabolic-engineering;numerical-simulations},
