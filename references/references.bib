  title = {Database {{Exploration}} for {{Microbial Information}}},
  booktitle = {Proceedings of the 3rd {{International Conference}} on {{Computer Science}} and {{Application Engineering}}},
  author = {Zepeda, Sergio and Estrada, Juan C. and Estrada, Daniel A.},
  date = {2019-10-22},
  series = {{{CSAE}} 2019},
  pages = {1--5},
  publisher = {{Association for Computing Machinery}},
  location = {{New York, NY, USA}},
  doi = {10.1145/3331453.3361670},
  url = {https://doi.org/10.1145/3331453.3361670},
  urldate = {2021-10-31},
  abstract = {Several countries on different continents have collections of microbial cultures; these collections have a lot of information, because a single microorganism can collect a great amount of information in several researches. Regularly, microbiology researchers want to explore databases to find new knowledge, relationships, similarities, etc. However, information systems with traditional HTML forms offer poor access to data and limited interaction. Therefore, new computer tools that allow interactive database exploration for this type of information are necessary. This paper presents a new approach to interaction in database exploration. This proposal uses a semantic representation to obtain a quick, efficient and intuitive way to access information. Our proposal is to transform the semantics contained in a conceptual scheme of a database in a structure composed by visual elements with meaning in the interface. This new way of interaction offers a field of research that can allow the creation of generic architectures for database exploration for use in various types of scientific databases.},
  isbn = {978-1-4503-6294-8},
  keywords = {Database Management,Information System and Applications,Scientific Databases}
}

