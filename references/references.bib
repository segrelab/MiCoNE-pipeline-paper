@article{kimYeastSyntheticBiology2015,
  title = {Yeast Synthetic Biology for the Production of Recombinant Therapeutic Proteins},
  author = {Kim, Hyunah and Yoo, Su Jin and Kang, Hyun Ah},
  date = {2015-02-01},
  journaltitle = {FEMS Yeast Research},
  shortjournal = {FEMS Yeast Research},
  volume = {15},
