@book{oneillCollaboratoryNotebookNetworked1994,
  title = {The {{Collaboratory Notebook}}: {{A Networked Knowledge-Building Environment}} for {{Project Learning}}},
  shorttitle = {The {{Collaboratory Notebook}}},
