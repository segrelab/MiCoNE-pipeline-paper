@article{dixitBenchmarking16SRRNA2021,
  title = {Benchmarking of {{16S rRNA}} Gene Databases Using Known Strain Sequences},
  author = {Dixit, Kunal and Davray, Dimple and Chaudhari, Diptaraj and Kadam, Pratik and Kshirsagar, Rudresh and Shouche, Yogesh and Dhotre, Dhiraj and Saroj, Sunil D},
