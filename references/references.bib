@article{pachecoMultidimensionalPerspectiveMicrobial2019,
  title = {A Multidimensional Perspective on Microbial Interactions},
  author = {Pacheco, Alan R and Segrè, Daniel},
  date = {2019-06-01},
  journaltitle = {FEMS Microbiology Letters},
  shortjournal = {FEMS Microbiology Letters},
  volume = {366},
  number = {11},
