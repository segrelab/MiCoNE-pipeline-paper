  author = {Opitz, Christiane A. and Litzenburger, Ulrike M. and Sahm, Felix and Ott, Martina and Tritschler, Isabel and Trump, Saskia and Schumacher, Theresa and Jestaedt, Leonie and Schrenk, Dieter and Weller, Michael and Jugold, Manfred and Guillemin, Gilles J. and Miller, Christine L. and Lutz, Christian and Radlwimmer, Bernhard and Lehmann, Irina and von Deimling, Andreas and Wick, Wolfgang and Platten, Michael},
